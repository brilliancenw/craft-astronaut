{
    "name": "brilliance/craft-launcher-assistant",
    "description": "AI-powered assistant for Craft CMS that integrates with the Launcher plugin",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "launcher",
        "assistant",
        "ai",
        "claude"
    ],
    "support": {
        "docs": "https://github.com/brilliancenw/craft-launcher-assistant/blob/main/README.md",
        "issues": "https://github.com/brilliancenw/craft-launcher-assistant/issues"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Brilliance",
            "homepage": "https://www.brilliancenw.com/"
        }
    ],
    "require": {
        "php": "^8.2",
        "craftcms/cms": "^5.0.0",
        "brilliance/craft-launcher": "^1.2.0"
    },
    "autoload": {
        "psr-4": {
            "brilliance\\launcherassistant\\": "src/"
        }
    },
    "extra": {
        "name": "Launcher Assistant",
        "handle": "launcher-assistant",
        "class": "brilliance\\launcherassistant\\LauncherAssistant",
        "developer": "Brilliance",
        "developerUrl": "https://www.brilliancenw.com/",
        "documentationUrl": "https://github.com/brilliancenw/craft-launcher-assistant/blob/main/README.md",
        "changelogUrl": "https://github.com/brilliancenw/craft-launcher-assistant/blob/main/CHANGELOG.md"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.4",
        "yiisoft/yii2-dev": "^2.0"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
